#include <iostream>
using namespace std;
class Parent{
    public:
    virtual void show() final
    {
      //  cout<<"Paret"<<endl;
    }
};
class Child:Parent{
    public:
    void show(){
        cout<<"Child"<<endl;
    }
};
int main()
{
    Parent p;
    Child c;
    c.show();
    return 0;
}
